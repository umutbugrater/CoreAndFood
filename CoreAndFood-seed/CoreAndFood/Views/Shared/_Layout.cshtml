

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>

    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <ul class="nav navbar-nav">
                    <li class="active"> <a href="#">CORE AND FOOD</a></li>
                </ul>
            </div>
            <ul class="nav navbar-nav">
                <li><a asp-action="Index" asp-controller="Category">Category</a></li>
                <li><a asp-action="Index" asp-controller="Food">Food</a></li>
                <li><a asp-action="Index" asp-controller="Chart">Google Pie Chart</a></li>
                <li><a asp-action="Index2" asp-controller="Chart">Google Column Chart</a></li>
                <li><a asp-action="Index3" asp-controller="Chart">Google Dynamic Chart</a></li>
                <li><a asp-action="Statistics" asp-controller="Chart">Statistics</a></li>
                <li><a asp-action="LogOut" asp-controller="Login">Log Out</a></li>
            </ul>
        </div>
    </nav>

    <div class="container">
        @RenderBody()
    </div>

</body>
</html>
